@model List<Disciplina>
@{
    ViewBag.Title = "Disciplina";
}

<h2>Disciplina</h2>



<table class="table table-striped">
    <thead>
        <tr>
            <td>Cod. Disciplina</td>
            <td>Nome do Aluno</td>
            <td>Nome da Disciplina</td>
            <td>Semestre</td>
            <td>Curso</td>
        </tr>
    </thead>
    <tbody>

        @foreach (var disc in @Model)
        {
            <tr>
                <td>@disc.IdDisciplina</td>
                <td>@ViewBag.NomeAluno</td>
                <td>@disc.Nome</td>
                <td>@disc.Semestre</td>
                <td>@disc.Curso</td>
            </tr>
        }

    </tbody>
</table>